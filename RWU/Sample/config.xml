    </plugin>
    <plugin name="cordova-background-geolocation" spec="file:../../13Aug2018/cordova-background-geolocation-master">
        <variable name="LICENSE" value="951cf89d476b225cb0874117118fdf0668beccaec7ef7b765273f5dd1bfa8f86" />
        <variable name="GOOGLE_API_VERSION" value="11.8.0" />
        <variable name="APPCOMPAT_VERSION" value="27.0.0" />
    </plugin>
    <plugin name="cordova-plugin-core-android-extensions" spec="^1.2.0" />
    <plugin name="cordova-plugin-image-resizer" spec="^1.0.0" />
